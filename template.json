{
  "package": {
    "scripts": {
      "build": "craco build",
      "eject": "react-scripts eject",
      "eslint": "eslint src",
      "eslint:fix": "eslint --fix src",
      "prepare": "husky install",
      "prettier": "prettier ./src --ignore-unknown",
      "prettier:write": "prettier src --write --ignore-unknown",
      "serve": "serve -s build",
      "start": "craco start",
      "stylelint": "stylelint src/**/*.{css,scss,sass}",
      "stylelint:fix": "stylelint --fix src/**/*.{css,scss,sass}",
      "test": "craco test"
    },
    "dependencies": {
      "@craco/craco": "7.1.0",
      "@emotion/react": "11.11.1",
      "@emotion/styled": "11.11.0",
      "@hookform/resolvers": "3.1.1",
      "@mui/icons-material": "5.11.16",
      "@mui/lab": "^5.0.0-alpha.107",
      "@mui/material": "5.13.6",
      "@mui/system": "5.13.6",
      "axios": "1.4.0",
      "i18next": "23.2.5",
      "i18next-browser-languagedetector": "7.1.0",
      "i18next-http-backend": "2.2.1",
      "joi": "17.9.2",
      "prop-types": "15.8.1",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-hook-form": "7.45.1",
      "react-i18next": "13.0.1",
      "react-router-dom": "6.14.0",
      "react-scripts": "5.0.1",
      "web-vitals": "3.3.2",
      "zustand": "4.3.8"
    },
    "devDependencies": {
      "@testing-library/jest-dom": "5.16.5",
      "@testing-library/react": "14.0.0",
      "@testing-library/user-event": "14.4.3",
      "@types/jest": "29.5.2",
      "@types/node": "20.3.2",
      "@types/react": "18.2.14",
      "@types/react-dom": "18.2.6",
      "eslint": "8.43.0",
      "eslint-config-alloy": "5.0.0",
      "eslint-config-react-app": "7.0.1",
      "husky": "8.0.3",
      "lint-staged": "13.2.3",
      "prettier": "2.8.8",
      "prettier-plugin-organize-imports": "3.2.2",
      "prettier-plugin-packagejson": "2.4.3",
      "serve": "14.2.0",
      "stylelint": "15.9.0",
      "stylelint-config-standard": "33.0.0",
      "typescript": "5.1.6"
    },
    "eslintConfig": {
      "extends": [
        "alloy",
        "alloy/typescript",
        "alloy/react",
        "react-app",
        "react-app/jest"
      ]
    },
    "stylelint": {
      "extends": "stylelint-config-standard"
    },
    "lint-staged": {
      "package.json": "prettier --write",
      "*.{js,jsx}": [
        "prettier --write --ignore-unknown",
        "eslint src --fix"
      ],
      "*.{ts,tsx}": [
        "prettier --parser typescript --write --ignore-unknown",
        "eslint src --fix"
      ],
      "*.{css,md}": "prettier --write --ignore-unknown"
    }
  },
  "overrides": {
    "react-scripts": {
      "typescript": "5.1.6"
    }
  }
}